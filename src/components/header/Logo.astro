---
import { Image } from 'astro:assets';
import logoIcon from './../../assets/logo.svg';

const { link = '/', alt = '' } = Astro.props;
---

<a href={link}>
    <Image src={logoIcon} width={100} height={44} alt='Logo'/>
</a>



<style lang="scss">
   img {
    width: 100px;
    height: 44px;
   }
</style>